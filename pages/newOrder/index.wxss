page {
  background: #f5f6f7;
}

.content {
  height: 100%;
}

.content .section {
  width: 100%;
  padding: 10px 15px;
  font-size: 14px;
  line-height: 24px;
  color: #333;
  background-color: #fff;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
}

.content .detePicker {
  width: 100%;
  padding: 10px 15px;
  font-size: 14px;
  line-height: 24px;
  color: #333;
  background-color: #fff;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
}
.content .detePicker::before {
  position: absolute;
  left: 7px;
  font-size: 14px;
  color: #f44;
  content: "*";
}

